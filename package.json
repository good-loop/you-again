{
  "name": "hooru",
  "version": "0.3.5",
  "description": "hooro - proncounced who-are-you is a login and authentication manager. It will handle name/password logins, plus oauth based Twitter/Facebook/etc logins.",
  "main": "./bin/hooru.js",
  "dependencies": {
    "jquery": "^3.1.1",
    "js-cookie": "^2.1.4",
    "sjtest": "^0.4.3"
  },  
  "keywords": [
    "login", "single signon", "auth"
  ],
  "author": "Daniel Winterstein",
  "license": "ISC",  
  "homepage": "https://github.com/winterstein/hooru#readme",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0"
  },
  "scripts": {
    "build": "babel src --out-dir bin",
    "watch": "babel src --out-dir bin --watch",
    "prepublish": "npm run build"
  }    
}
